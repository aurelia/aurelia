    "@aurelia/metadata": "dev",
    "@aurelia/route-recognizer": "dev",
    "@aurelia/router": "dev",
    "@aurelia/runtime-html-browser": "dev",
    "@aurelia/runtime-html": "dev",
    "@aurelia/runtime": "dev",
    "@aurelia/scheduler-dom": "dev",
    "@aurelia/scheduler": "dev",
    "aurelia": "dev"
  },
  "devDependencies": {
    "@types/node": "^12.12.21",
    "au": "0.6.0",
    "clean-webpack-plugin": "^3.0.0",
    "html-loader": "^0.5.5",
