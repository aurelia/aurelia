  "version": "0.7.0",
  "scripts": {
    "watch": "webpack-dev-server --no-inline",
    "build": "rimraf dist && webpack",
    "serve": "http-server -c-1 -p 9000 dist",
    "start:au": "node -r esm ../../packages/au/dist/esnext/index.js dev au.conf.js",
    "start:au1": "node -r esm ../../packages/au/dist/esnext/index.js dev --server.root ./dist/ --server.port 443 --server.useHttp2 true --server.key ./ssl/key.pem --server.cert ./ssl/cert.pem"
  },
  "dependencies": {
    "@aurelia/debug": "dev",
