  "scripts": {
    "test": "./node_modules/.bin/cross-env TS_NODE_PROJECT=\"tsconfig-tsnode.json\" ./node_modules/.bin/karma start --browsers=ChromeHeadless --single-run=true --auto-watch=false --no-info=true --log-level=error --coverage=true",
    "test:debugger": "./node_modules/.bin/cross-env TS_NODE_PROJECT=\"tsconfig-tsnode.json\" ./node_modules/.bin/karma start --browsers=ChromeDebugging --single-run=false --auto-watch=true --no-info=false",
