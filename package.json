    "test:debug": "npm run test -- --component=debug",
    "test:debug:debugger": "npm run test:debugger -- --component=debug",
    "test:debug:watch": "npm run test:watch -- --component=debug",
    "test:jit": "npm run test -- --component=jit",
    "test:jit:debugger": "npm run test:debugger -- --component=jit",
    "test:jit:watch": "npm run test:watch -- --component=jit",
    "transpile": "tsc src/compile.ts --outDir scripts",
    "build": "rollup -c compiler.rollup.config.js",
    "test:compile": "node compiler/test/compile",
    "start": "npm run build && npm run test"
  },
