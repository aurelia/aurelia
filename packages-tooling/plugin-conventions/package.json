    "publish:dev": "npm publish --tag dev",
    "publish:latest": "npm publish --tag latest",
    "rollup": "rollup -c",
    "postrollup": "tsc --emitDeclarationOnly",
    "build:packages-tooling": "npm run rollup"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@aurelia/kernel": "2.0.0-beta.20",
    "@aurelia/metadata": "2.0.0-beta.20",
    "@aurelia/platform": "2.0.0-beta.20",
    "@aurelia/runtime": "2.0.0-beta.20",
    "@aurelia/runtime-html": "2.0.0-beta.20",
    "modify-code": "^2.1.3",
    "parse5": "^7.1.2",
    "typescript": "^5.4.2"
  },
  "devDependencies": {
    "@types/node": "^14.18.14"
  },
  "engines": {
    "node": ">=14.17.0"
  }
}