    "@aurelia/router-lite": "2.0.0-beta.7",
    "@aurelia/runtime-html": "2.0.0-beta.7",
    "@aurelia/runtime": "2.0.0-beta.7",
    "@aurelia/ui-virtualization": "2.0.0-beta.7"
  },
  "devDependencies": {
    "webpack-cli": "^4.9.2",
    "webpack": "^5.72.0",
