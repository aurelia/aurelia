    "@types/mocha": "^5.2.5",
    "@types/node": "^8.10.22",
    "@types/selenium-webdriver": "^3.0.10",
    "browserify": "^16.2.2",
    "chai": "^4.1.2",
    "chromedriver": "^2.41.0",
    "gulp": "^3.9.1",
    "http-server": "^0.11.1",
    "mocha": "^5.2.0",
    "path": "^0.12.7",
    "rimraf": "^2.6.2",
    "selenium-webdriver": "^4.0.0-alpha.1",
    "ts-node": "^7.0.1",
    "tsify": "^4.0.0",
