{
  "name": "@aurelia/jit",
  "version": "0.1.1",
  "main": "dist/umd/index.js",
  "module": "dist/es2015/index.js",
  "jsnext:main": "dist/es2015/index.js",
  "typings": "dist/umd/index.d.ts",
  "types": "dist/umd/index.d.ts",
  "license": "MIT",
  "homepage": "https://aurelia.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/aurelia/aurelia"
  },
  "bugs": {
    "url": "https://github.com/aurelia/aurelia/issues"
  },
  "keywords": [
    "aurelia",
    "jit"
  ],
  "scripts": {
    "lint": "cross-env tslint --project tsconfig-test.json -e \"test/**\" -t stylish",
    "test": "cross-env TS_NODE_PROJECT=\"../../scripts/tsconfig.json\" karma start ../../scripts/karma.conf.ts --package=jit --browsers=ChromeHeadless --single-run --coverage",
    "test:watch": "cross-env TS_NODE_PROJECT=\"../../scripts/tsconfig.json\" karma start ../../scripts/karma.conf.ts --package=jit --browsers=ChromeHeadless --coverage",
    "test:debugger": "cross-env TS_NODE_PROJECT=\"../../scripts/tsconfig.json\" karma start ../../scripts/karma.conf.ts --package=jit --browsers=ChromeDebugging",
    "build:amd": "tsc -b tsconfig/amd.json",
    "build:commonjs": "tsc -b tsconfig/commonjs.json",
    "build:es2015": "tsc -b tsconfig/es2015.json",
    "build:esnext": "tsc -b tsconfig/esnext.json",
    "build:system": "tsc -b tsconfig/system.json",
    "build:umd": "tsc -b tsconfig/umd.json",
    "build-all": "run-p build:*",
    "dev": "tsc -b tsconfig/umd.json -w",
    "publish:local": "npm pack"
  },
  "dependencies": {
    "@aurelia/kernel": "^0.1.1",
    "@aurelia/runtime": "^0.1.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/karma": "^1.7.5",
    "@types/mocha": "^5.2.5",
    "@types/node": "^8.10.22",
    "@types/sinon": "^5.0.1",
    "@types/sinon-chai": "^3.2.0",
    "@types/webpack": "^4.4.8",
    "@types/webpack-env": "^1.13.6",
    "aurelia-pal-browser": "^1.8.0",
    "aurelia-polyfills": "^1.3.0",
    "chai": "^4.1.2",
    "cross-env": "^5.2.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "karma": "^2.0.5",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-coverage-istanbul-reporter": "^2.0.1",
    "karma-firefox-launcher": "^1.1.0",
    "karma-junit-reporter": "^1.2.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.0",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.3",
    "path": "^0.12.7",
    "sinon": "^6.1.4",
    "sinon-chai": "^3.2.0",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.0",
    "tsconfig-paths": "^3.5.0",
    "tslint": "^5.11.0",
    "tslint-microsoft-contrib": "^5.1.0",
    "tslint-no-unused-expression-chai": "^0.1.3",
    "typescript": "^3.0.1",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  }
}
