{
  "extends": "../tslint.json",
  "linterOptions": {
    "exclude": []
  },
  "defaultSeverity": "warning",
  "rules": {
    "mocha-no-side-effect-code": false,
    "no-any": false,
    "no-big-function": false,
    "no-empty": false,
    "no-implicit-dependencies": false,
    "no-invalid-template-strings": false,
    "typedef": false
  }
}
