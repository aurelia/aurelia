{
  "extends": [
    ":separateMajorReleases",
    ":combinePatchMinorReleases",
    ":ignoreUnstable",
    ":renovatePrefix",
    ":semanticPrefixFixDepsChoreOthers",
    ":updateNotScheduled",
    ":automergeDisabled",
    ":disableRateLimiting",
    ":maintainLockFilesMonthly",
    "helpers:disableTypesNodeMajor",
    "schedule:weekly",
    "group:all"
  ],
  "ignorePaths": [
    "**/node_modules/**"
  ],
  "node": {
    "supportPolicy": ["lts"]
  },
  "packageRules": [
    {
      "depTypeList": ["devDependencies"],
      "rangeStrategy": "bump"
    },
    {
      "depTypeList": ["dependencies", "engines", "peerDependencies"],
      "rangeStrategy": "widen"
    },
    {
      "packageNames": ["bootstrap"],
      "allowedVersions": "^3.3.6"
    },
    {
      "packageNames": ["chromedriver"],
      "allowedVersions": "^74.0.0"
    },
    {
      "packageNames": ["karma"],
      "allowedVersions": "~4.1.0"
    },
    {
      "packageNames": ["http-server"],
      "allowedVersions": "0.9.0"
    },
    {
      "packageNames": ["mocha"],
      "allowedVersions": "~6.1.4"
    },
    {
      "packageNames": ["pixi.js", "@types/pixi.js"],
      "allowedVersions": "~4.8.8"
    }
  ]
}
