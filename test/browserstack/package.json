{
  "name": "e2e",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.bs.json && run-p build:*",
    "build:todos": "webpack --app todos",
    "build:select": "webpack --app select",
    "start": "http-server -c-1 -p 9000 .",
    "e2e": "npm run build && cross-env TS_NODE_PROJECT=\"tsconfig.bs.json\" wdio browserstack.conf.js",
    "e2e:compat": "cross-env BS_COMPAT_CHECK=true npm run e2e"
  },
  "dependencies": {
    "@aurelia/jit": "0.4.0",
    "@aurelia/jit-html": "0.4.0",
    "@aurelia/jit-html-browser": "0.4.0",
    "@aurelia/kernel": "0.4.0",
    "@aurelia/runtime": "0.4.0",
    "@aurelia/runtime-html": "0.4.0",
    "@aurelia/runtime-html-browser": "0.4.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.5",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.7",
    "@wdio/browserstack-service": "^5.16.5",
    "@wdio/cli": "^5.16.7",
    "@wdio/local-runner": "^5.16.7",
    "@wdio/mocha-framework": "^5.16.5",
    "@wdio/selenium-standalone-service": "^5.16.5",
    "@wdio/spec-reporter": "^5.16.5",
    "@wdio/sync": "^5.16.5",
    "browserstack-local": "^1.4.2",
    "chai": "^4.2.0",
    "chalk": "^3.0.0",
    "cross-env": "^6.0.3",
    "fancy-log": "^1.3.3",
    "html-loader": "^0.5.5",
    "http-server": "0.9.0",
    "mocha": "~6.1.4",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "rimraf": "^3.0.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.5.0",
    "tsconfig-paths": "^3.9.0",
    "tslib": "^1.10.0",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  }
}
