    "@aurelia/scheduler": "0.7.0",
    "promise-polyfill": "^8.1.3"
  },
  "devDependencies": {
    "@aurelia/http-server": "0.7.0",
    "htmlhint": "^0.11.0",
    "rimraf": "^3.0.0",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "html-webpack-plugin": "^4.5.0",
    "style-loader": "^1.1.1",
