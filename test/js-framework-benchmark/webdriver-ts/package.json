    "@types/selenium-webdriver": "4.0.6",
    "@types/semver": "6.2.0",
    "@types/yargs": "13.0.3",
    "dockerode": "^3.0.2",
    "rimraf": "^3.0.0",
    "ts-node": "^8.8.2",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "cross-env": "^6.0.3",
    "chrome-launcher": "^0.12.0",
    "chromedriver": "^77.0.0",
    "dot": "^1.1.2",
    "jstat": "^1.9.2",
    "lighthouse": "^5.6.0",
    "lodash": "^4.17.15",
    "ramda": "^0.26.1",
    "selenium-webdriver": "^4.0.0-alpha.5",
    "yargs": "^14.2.0 || ^15.0.0",
    "npm-check-updates": "^3.2.1 || ^4.0.0",
    "semver": "^6.3.0 || ^7.0.0"
  }
